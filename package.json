{
  "name": "student-tracker",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "babel-watch --extensions .ts src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "pm2:start": "pm2 start processes.config.js --env production",
    "pm2:stop": "pm2 stop processes.config.js --env production",
    "pm2:delete": "pm2 delete processes.config.js --env production",
    "pm2:restart": "pm2 restart processes.config.js --env production",
    "test": "pnpm run test:lint && pnpm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "pnpm run test:unit",
    "prisma:migrate": "pnpx prisma migrate dev --name init",
    "prisma:push": "prisma db push",
    "prisma:generate": "pnpx prisma generate",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prisma:studio": "pnpx prisma studio",
    "docker:infra:up": "docker compose up -d",
    "docker:infra:dowm": "docker compose down",
    "docker:build": "docker compose -f docker-compose.build.yml build",
    "docker:start": "docker compose -f docker-compose.build.yml up -d",
    "docker:push": "docker push docker.io/sanjumsanthosh/student-tracker:latest",
    "docker:monitor:up": "docker compose -f docker-compose.monitor.yaml up -d",
    "docker:monitor:down": "docker compose -f docker-compose.monitor.yaml down"
  },
